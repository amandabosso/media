programa
 {
  funcao inicio() 
  {
      // estrutura de repetição
      inteiro  contador = 1
      inteiro fim = 0

      escreva (" Informe o número  de vezes: ")
      leia (fim)
  
      escreva ("\n")
      enquanto ( contador <= fim){
        escreva ("Estou estudando programação", "\n")
        contador = contador + 1

      }
       
  }
}

