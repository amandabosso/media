programa
 {
  funcao inicio() 
  {
      // estrutura de repetição
      inteiro contador = 0

      inteiro fim = 0
      escreva (" Informe o número para o fim da contage: ")
      leia (fim)
      escreva ("\n")
  
      enquanto ( contador <= fim ){
        escreva ("numero: ", contador, "\n")
        contador = contador + 1

      }
          escreva ( " saiu do laço")
  }
}
